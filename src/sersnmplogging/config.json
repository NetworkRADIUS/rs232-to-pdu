{
    "version": 1,
    "formatters": {
        "stdout": {
            "format": "%(message)s"
        },
        "default": {
            "format": "%(asctime)s - %(name)s - %(levelname)s : At Line %(lineno)s of %(module)s :: %(message)s"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "stdout",
            "stream": "ext://sys.stdout"
        },
        "stderr": {
            "class": "logging.StreamHandler",
            "level": "ERROR",
            "formatter": "stdout",
            "stream": "ext://sys.stderr"
        },
        "default": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "default",
            "filename": "output.log",
            "maxBytes": 10240000,
            "backupCount": 10
        }
    },
    "loggers": {
        "": {
            "level": "DEBUG",
            "handlers": []
        },
        "nwkrad": {
            "level": "DEBUG",
            "handlers": ["stdout", "stderr","default"]
        }
    },
    "disable_existing_loggers": false
}