[Unit]
Description = A tool to convert ser232 string commands to SNMP SET commands for Tripplite PDUs
Documentation = https://github.com/NetworkRADIUS/rs232-to-tripplite-pdu/blob/main/README.md

[Service]
Type=simple
Restart=on-watchdog
ExecStart=/usr/local/ser2snmp/venv/bin/python -m ser2snmp

[Install]
WantedBy=multi-user.target